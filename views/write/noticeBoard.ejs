    </head>
    <body>
        <%- include('../header.html') %>

        <div class="content_wrap">
            <div class="content-header">
                <!-- Page Name -->
                <h3>공지사항</h3>
            </div>
            <aside class="leftmenubar">
                <h2>공지사항</h2>
                <ul>
                    <li><a href="/board/list/notice/10" class="current">공지사항</a></li>
                </ul>
            </aside>
            <section id="cont" class="content">
                <form class="writeform" id="writeform" method="post" action="">
                    <div class="write-wrap">
                        <div>
                            <input type="text" name="content_tit" id="content_tit" placeholder="제목을 입력하여주세요.">
                            <input type="hidden" id="content_user" name="content_user" value="<%= SD.id %>">
                            <input type="hidden" id="boardName" value="<%= name %>">
                        </div>
                        <div id="selLang">
                            <span>구분 : </span>
                            <select name="content_kind" id="content_lang">
                                <option value="notice">공지사항</option>
                            </select>
                        </div>
                        <textarea name="content_txt" id="content_txt"></textarea>
                        <div class="btn_wrap">
                            <input type="button" id="btnCancle" class="inp_btn" value="취소">
                            <input type="button" id="btnWrite" class="inp_btn" value="등록">
                        </div>
                    </div>
                </form>
            </section>
        </div>
        <footer class="page">
            <!-- Footer Part -->
            <p>Copyright 2022. MinnSeShin. All Right Reserved.</p>
        </footer>
    </body>
